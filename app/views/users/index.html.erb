<%= form_tag users_path, method:"get" do %>
	<%= field_set_tag do%>
		<%= label_tag 'Email'%>
		<%= text_field_tag 'email', nil, class: "email_login"%>
		<%= label_tag 'Password'%>
		<%= password_field_tag 'password', nil, class: "email_login"%>
		<%= button_tag 'Log In' %>
	<% end %>
<% end %>

<% @stuff.each do |person| %>
	<%=person.first_name %></br>
	<%=person.last_name %></br>
	<%=person.email %></br>
	<%=person.password %>

	<% end %>
